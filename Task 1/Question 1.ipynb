    "N, X = input().split()\n",
    "N = int(N)\n",
    "X = int(X)\n",
    "marks = []\n",
    "for i in range(X):\n",
    "    m = input().split()\n",
    "    m = map(float, m)\n",
    "    marks.append(m)\n",
    "    \n",
    "for i in zip(*marks):\n",
    "    avg = (sum(i)/len(i))\n",
    "    print(avg)"
   ]
  }
